{
  "routes": [
    {
      "routePath": "/api/auth/login",
      "mountPath": "/api/auth",
      "method": "POST",
      "module": [
        "api/auth/login.ts:onRequestPost"
      ]
    },
    {
      "routePath": "/api/auth/register",
      "mountPath": "/api/auth",
      "method": "POST",
      "module": [
        "api/auth/register.ts:onRequestPost"
      ]
    },
    {
      "routePath": "/api/storage/upload",
      "mountPath": "/api/storage",
      "method": "POST",
      "module": [
        "api/storage/upload.ts:onRequestPost"
      ]
    },
    {
      "routePath": "/api/storage/:path*",
      "mountPath": "/api/storage",
      "method": "GET",
      "module": [
        "api/storage/[[path]].ts:onRequestGet"
      ]
    },
    {
      "routePath": "/api/users",
      "mountPath": "/api/users",
      "method": "GET",
      "module": [
        "api/users/index.ts:onRequestGet"
      ]
    },
    {
      "routePath": "/api/users",
      "mountPath": "/api/users",
      "method": "POST",
      "module": [
        "api/users/index.ts:onRequestPost"
      ]
    }
  ],
  "baseURL": "/"
}